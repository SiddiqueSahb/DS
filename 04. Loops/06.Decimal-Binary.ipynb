   ],
   "source": [
    "num=int(input())\n",
    "bin = 0\n",
    "place = 1 \n",
    "while num!=0:\n",
    "    rem = num % 2\n",
    "    num = num // 2\n",
    "    bin = bin + rem * place\n",
    "    place = place * 10\n",
    "print(bin)\n",
    "    "
   ]
